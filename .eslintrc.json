{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true,
    "node": true
  },
  "extends": "prettier",
  "plugins": ["prettier"],
  "rules": {
    "yoda": "error",
    "eqeqeq": "error",
    "no-empty": "error",
    "use-isnan": "error",
    "no-dupe-args": "error",
    "no-dupe-keys": "error",
    "no-ex-assign": "error",
    "no-cond-assign": "error",
    "no-func-assign":"error",
    "no-unused-vars":"error",
    "no-dupe-else-if": "error",
    "no-duplicate-case": "error",
    "no-unsafe-negation": "error",
    "no-irregular-whitespace": "error"
}

}
